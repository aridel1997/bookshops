<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    >

  <Dialog
      id="idAddAuthor"
      title="Add author">
      <TextArea value="{viewModel>/sFrameVideo}" rows="8" />
   

    <beginButton>
      <Button
          text="{ShowVideo}"
          type="Emphasized"
          press="onShowVideo" />
    </beginButton>
    <endButton>
      <Button
          text="Cancel"
          press="onCloseDialog" />
    </endButton>
  </Dialog>
</core:FragmentDefinition>